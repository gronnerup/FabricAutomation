{
    "feature_name" : "*{feature_name}-{layer_name}",
    "capacity_name": "MyCapacity",
    "git_settings": {
         "gitProviderDetails": {
            "gitProviderType": "AzureDevOps",
            "organizationName": "MyAzureDevOpsOrg",
            "projectName": "MyAzureDevOpsProject",
            "repositoryName": "MyAzureDevOpsRepository"
        },
        "myGitCredentials" : {
            "source": "ConfiguredConnection",
            "connection_name" : "MyGitConnectionName-ADO"
        }
    },
    "permissions": {
        "admin": [
            {"type": "Group", "id": "00000000-0000-0000-0000-000000000000"}
        ],
        "contributor": [
            {"type": "User", "id": "00000000-0000-0000-0000-000000000000"}
        ]
    },
    "layers" : {
        "Prepare": { 
            "spark_settings": {
                "pool": {
                    "starterPool":{
                        "maxExecutors": 1,
                        "maxNodeCount": 1
                    }
                }
            },
            "git_directoryName": "solution/prepare",
            "git_synchronize_on_commit": true
        },
        "Ingest": { 
            "git_directoryName": "solution/ingest",
            "git_synchronize_on_commit": false
        },
        "Orchestrate": {
            "git_directoryName": "solution/orchestrate",
            "git_synchronize_on_commit": false
        }
    }
}